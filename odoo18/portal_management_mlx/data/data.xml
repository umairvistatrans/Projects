            <field name="padding">5</field>
        </record>

        <!-- Sequence for Product -->
        <record id="seq_submission_request_product" model="ir.sequence">
            <field name="name">Submission Request - Product</field>
            <field name="code">submission.request.product</field>
            <field name="prefix">PRD-</field>
            <field name="padding">5</field>
        </record>

        <!-- Sequence for Category -->
        <record id="seq_submission_request_category" model="ir.sequence">
            <field name="name">Submission Request - Category</field>
            <field name="code">submission.request.category</field>
            <field name="prefix">CAT-</field>
            <field name="padding">5</field>
        </record>

    </data>
