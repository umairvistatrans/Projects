        <!-- Sequence for Supplier -->
        <record id="seq_submission_request_supplier" model="ir.sequence">
            <field name="name">Submission Request - Supplier</field>
            <field name="code">submission.request.supplier</field>
            <field name="prefix">SUP-</field>
            <field name="padding">5</field>
        </record>

        <!-- Sequence for Factory -->
        <record id="seq_submission_request_factory" model="ir.sequence">
