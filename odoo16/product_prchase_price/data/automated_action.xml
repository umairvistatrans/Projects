<?xml version="1.0"?>
<odoo>
    <data noupdate="1">
        <record id="update_product_price_on_receipt_validation" model="base.automation">
            <field name="name">Update Product Price on Receipt Validation</field>
            <field name="model_id" ref="stock.model_stock_move"/>
            <field name="state">code</field>
            <field name="code" eval="'records._update_product_price_on_receipt_validation()'"/>
            <field name="trigger">on_write</field>
            <field name="filter_pre_domain">[('state', '=', 'done'), ('picking_id.state', '=', 'done'), ('picking_id.picking_type_code', '=', 'incoming')]</field>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>
