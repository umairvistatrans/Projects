<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="check_validity" forcecreate='True' model="ir.cron">
            <field name="name">Subscription Validity</field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_subscription_config"/>
            <field name="state">code</field>
            <field name="code">model.check_subscription_expiry()</field>
        </record>

    </data>
</odoo>