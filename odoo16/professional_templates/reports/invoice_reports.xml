<odoo>
    <data>
        <!--inherits the original invoice report  and override its name -->
        <record id="account.account_invoices" model="ir.actions.report">
            <field name="model">account.move</field>
            <field name="name">Invoices</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">professional_templates.report_invoice_with_payments</field>
            <field name="print_report_name">(object._get_report_base_filename())</field>
        </record>
        <record id="account.account_invoices_without_payment" model="ir.actions.report">
            <field name="model">account.move</field>
            <field name="name">Invoices without Payment</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">professional_templates.report_invoice</field>
            <field name="print_report_name">(object._get_report_base_filename())</field>
        </record>
    </data>
</odoo>
