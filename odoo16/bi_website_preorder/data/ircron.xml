<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <record id="ir_cron_preorder_product_notification" model="ir.cron">
        <field name="name">Pre-order Product Available</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="model_id" ref="model_website_preorder"/>
        <field name="code">model.auto_mail_notification()</field>
        <field name="state">code</field>
    </record>
</odoo>
