<?xml version="1.0" encoding="UTF-8"?>
<templates id="pos_stock_quantity.template" inherit_id="point_of_sale.template">

    <t  t-name="OLClosePosPopup" t-inherit="point_of_sale.ClosePosPopup" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[@t-on-click='confirm']" position="replace">
            <div class="button highlight" t-on-click="confirm" t-if="env.pos.get_cashier().role == 'manager'">Close Session</div>
        </xpath>
    </t>

</templates>