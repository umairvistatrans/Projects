<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="view_inherit_pos_config_form_00" model="ir.ui.view">
            <field name="name">view.inherit.pos.config.form</field>
            <field name="model">pos.config</field>
            <field name="inherit_id" ref="point_of_sale.pos_config_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='name']" position="after">
                    <group>
                          <field name="trn_name" string="TRN Name"/>
                          <field name="whatsapp" string="Whatsapp"/>
                          <field name="snapchat" string="snapchat"/>
                          <field name="instagram" string="Instagram"/>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</odoo>