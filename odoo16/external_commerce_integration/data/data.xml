<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="0">


     <record id="action_credit_note" model="ir.actions.server">
            <field name="name">chnage state</field>
            <field name="model_id" ref="external_commerce_integration.model_stock_picking"/>
            <field name="binding_model_id" ref="external_commerce_integration.model_stock_picking"/>
            <field name="state">code</field>
            <field name="code">
                if records:
                action = records.button_custom_action()
            </field>
        </record>


    <record id="product_product_payment_charge" model="product.product">
        <field name="name">Payment charges</field>
        <field name="default_code">Payment_Charge_001</field>
        <field name="type">service</field>
        <field name="sale_ok" eval="False"/>
        <field name="purchase_ok" eval="False"/>
        <field name="list_price">0.0</field>
    </record>
    <record id="ir_cron_updating_product_template" model="ir.cron">
        <field name="name">Update Product Quantity on Products</field>
        <field name="interval_number">5</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall" />
        <field eval="True" name="active" />
        <field name="model_id" ref="model_product_template" />
        <field name="state">code</field>
        <field name="code">model.action_refresh_qty()</field>
    </record>
    <record id="ir_cron_start_sync_php_all_1" model="ir.cron">
        <field name="name">Sync Brand</field>
        <field name="interval_number">6</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall" />
        <field eval="False" name="active" />
        <field name="model_id" ref="external_commerce_integration.model_res_sync_table" />
        <field name="state">code</field>
        <field name="code">model.sync_brand()</field>
    </record>

    <record id="ir_cron_start_sync_php_all_2" model="ir.cron">
        <field name="name">Sync Categ</field>
        <field name="interval_number">6</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall" />
        <field eval="False" name="active" />
        <field name="model_id" ref="external_commerce_integration.model_res_sync_table" />
        <field name="state">code</field>
        <field name="code">model.sync_categ()</field>
    </record>


    <record id="ir_cron_start_sync_php_all_3" model="ir.cron">
        <field name="name">Sync Product</field>
        <field name="interval_number">9</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall" />
        <field eval="False" name="active" />
        <field name="model_id" ref="external_commerce_integration.model_res_sync_table" />
        <field name="state">code</field>
        <field name="code">model.sync_product()</field>
    </record>


    <record id="ir_cron_start_sync_php_all_4" model="ir.cron">
        <field name="name">Sync Customer</field>
        <field name="interval_number">11</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall" />
        <field eval="False" name="active" />
        <field name="model_id" ref="external_commerce_integration.model_res_sync_table" />
        <field name="state">code</field>
        <field name="code">model.sync_customer()</field>
    </record>


    <record id="ir_cron_start_sync_php_all_5" model="ir.cron">
        <field name="name">Sync Orders</field>
        <field name="interval_number">15</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall" />
        <field eval="False" name="active" />
        <field name="model_id" ref="external_commerce_integration.model_res_sync_table" />
        <field name="state">code</field>
        <field name="code">model.sync_orders()</field>
    </record>


    <record id="product_product_discount" model="product.product">
            <field name="name">Discount Charges</field>
            <field name="default_code">DISC_001</field>
            <field name="type">service</field>
            <field name="categ_id" ref="product.product_category_all"/>
            <field name="sale_ok" eval="False"/>
            <field name="purchase_ok" eval="False"/>
            <field name="list_price">0.0</field>
    </record>


</odoo>
